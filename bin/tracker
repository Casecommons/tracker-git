#!/usr/bin/env ruby
begin
  require 'tracker-git'
rescue LoadError
  require 'rubygems'
  require 'tracker-git'
end

tracker_token = ENV['tracker_token']
project_id = ENV['tracker_project_id']

project = Tracker::Project.new(tracker_token, project_id)
git = Tracker::Git.new
deliverer = Tracker::Deliverer.new(project, git)

deliverer.mark_as_delivered
